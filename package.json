{
  "name": "countrymap",
  "version": "1.1.0",
  "description": "A lightweight TypeScript package providing ISO-3611-1 countries data.",
  "keywords": [
    "countries",
    "country",
    "world",
    "iso"
  ],
  "license": "MIT",
  "author": "Zoheir Benghalem <kayuxx@tutanota.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kayuxx/countrymap"
  },
  "homepage": "https://github.com/kayuxx/countrymap",
  "bugs": "https://github.com/kayuxx/countrymap/issues",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "files": [
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit && eslint",
    "prepare": "tsup && tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "20.16.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3",
    "tsup": "8.2.4",
    "typescript": "latest",
    "vitest": "2.0.5"
  }
}
